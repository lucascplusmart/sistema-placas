{
  "version": 2,
  "builds": [
    { "src": "./frontend/package.json", "use": "@vercel/node" },
    { "src": "./backend/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
